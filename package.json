{
  "name": "trek",
  "version": "0.0.0",
  "description": "Fast Async Web Framework For Modern Node.js",
  "author": "Fangdun Cai <cfddream@gmail.com> (https://github.com/fundon)",
  "homepage": "https://trekjs.com/",
  "repository": "trekjs/trek",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "async/await",
    "es6",
    "framework",
    "middleware",
    "mvc",
    "rest",
    "server",
    "trek",
    "web"
  ],
  "scripts": {
    "bench": "cd benchmarks; make all",
    "build": "rm -rf dist; babel lib --out-dir dist",
    "lint": "eslint lib",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.5",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-preset-es2016": "6.11.3"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "lib"
  ],
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs",
      "transform-export-extensions"
    ],
    "presets": [
      "es2016"
    ]
  },
  "dependencies": {
    "accepts": "1.3.3",
    "on-finished": "2.3.0",
    "parseurl": "1.3.1",
    "range-parser": "1.2.0",
    "trek-router": "1.1.0",
    "type-is": "1.6.13",
    "vary": "1.1.0"
  }
}
