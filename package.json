{
  "name": "trek",
  "version": "0.0.0",
  "description": "Fast Async Web Framework For Modern Node.js",
  "author": "Fangdun Cai <cfddream@gmail.com> (https://github.com/fundon)",
  "homepage": "https://trekjs.com/",
  "repository": "trekjs/trek",
  "license": "MIT",
  "main": "dist/index.js",
  "keywords": [
    "async",
    "async/await",
    "framework",
    "middleware",
    "mvc",
    "rest",
    "server",
    "trek",
    "web"
  ],
  "scripts": {
    "bench": "cd benchmarks; make all",
    "build": "rm -rf dist; babel lib --out-dir dist",
    "clean": "rm -rf coverage .nyc_output",
    "lint": "xo",
    "prepublish": "npm run build",
    "test": "xo && nyc --reporter=lcov ava -r babel-register",
    "test:watch": "xo && nyc --reporter=lcov ava -r babel-register --watch"
  },
  "dependencies": {
    "accepts": "1.3.3",
    "content-type": "1.0.2",
    "delegate-proxy": "1.1.0",
    "fresh": "0.3.0",
    "on-finished": "2.3.0",
    "parseurl": "1.3.1",
    "range-parser": "1.2.0",
    "trek-router": "1.1.0",
    "type-is": "1.6.13",
    "vary": "1.1.0"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "6.11.5",
    "babel-plugin-transform-export-extensions": "6.8.0",
    "babel-preset-es2016": "6.11.3",
    "babel-register": "6.11.6",
    "nyc": "7.1.0",
    "xo": "0.16.0"
  },
  "engines": {
    "node": ">= 6"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "README.md",
    "dist",
    "lib"
  ],
  "babel": {
    "plugins": [
      "transform-async-to-generator",
      "transform-es2015-modules-commonjs",
      "transform-export-extensions"
    ],
    "presets": [
      "es2016"
    ]
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "es6"
    ],
    "semicolon": false,
    "space": true,
    "rules": {
      "babel/arrow-parens": [
        2,
        "as-needed"
      ],
      "babel/object-curly-spacing": 0,
      "curly": 0,
      "import/no-named-as-default": 0,
      "max-lines": [
        "error",
        500
      ],
      "no-warning-comments": [
        "error",
        {
          "terms": [
            "todo",
            "fixme"
          ],
          "location": "anywhere"
        }
      ],
      "space-before-function-paren": [
        2,
        "always"
      ],
      "yoda": 0
    }
  }
}
